{
	"name": "@alexaegis/husky",
	"description": "Husky, lint-staged and commitlint stuff",
	"version": "0.0.1",
	"license": "MIT",
	"author": {
		"email": "alexaegis@gmail.com",
		"name": "AlexAegis",
		"url": "https://www.github.com/AlexAegis"
	},
	"homepage": "https://www.github.com/AlexAegis/js#readme",
	"repository": "github:AlexAegis/js",
	"bugs": {
		"url": "https://www.github.com/AlexAegis/js/issues"
	},
	"keywords": [
		"husky",
		"lint-staged",
		"commitlint",
		"link",
		"symlink",
		"ts",
		"typescript",
		"js",
		"javascript"
	],
	"private": false,
	"config": {
		"engine-strict": false
	},
	"scripts": {
		"format": "prettier --write .",
		"lint:es": "eslint --max-warnings=0 --fix .",
		"lint:format": "prettier --check ."
	},
	"sideEffects": false,
	"main": "./index.js",
	"module": "./index.es.js",
	"exports": {
		".": {
			"types": "./index.d.ts",
			"node": "./index.js",
			"require": "./index.js",
			"import": "./index.es.js",
			"default": "./index.es.js"
		}
	},
	"publishConfig": {
		"access": "public"
	},
	"dependencies": {
		"@alexaegis/tools": "*",
		"husky": "^8.0.2"
	},
	"devDependencies": {
		"@types/node": "^18.11.9",
		"typescript": "4.9.3",
		"@alexaegis/vite": "*"
	}
}
