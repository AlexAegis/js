{
	"name": "@alexaegis/vite",
	"description": "My vite config",
	"version": "0.0.1",
	"license": "MIT",
	"author": {
		"email": "alexaegis@gmail.com",
		"name": "AlexAegis",
		"url": "https://www.github.com/AlexAegis"
	},
	"homepage": "https://www.github.com/AlexAegis/js#readme",
	"repository": "github:AlexAegis/js",
	"bugs": {
		"url": "https://www.github.com/AlexAegis/js/issues"
	},
	"keywords": [
		"vite",
		"ts",
		"typescript",
		"js",
		"javascript"
	],
	"private": false,
	"config": {
		"engine-strict": false
	},
	"scripts": {
		"prebuild": "vite build",
		"format": "prettier --write .",
		"lint:es": "eslint --max-warnings=0 .",
		"lint:format": "prettier --check .",
		"postinstall": "node --require ts-node/register ./src/bin/postinstall.ts # autogenerated"
	},
	"sideEffects": false,
	"publishConfig": {
		"access": "public"
	},
	"type": "module",
	"dependencies": {
		"vite": "3.2.2",
		"vite-plugin-static-copy": "0.11.1",
		"vite-plugin-dts": "^1.6.6",
		"@sveltejs/vite-plugin-svelte": "^1.1.0",
		"fs-extra": "^10.1.0"
	},
	"peerDependencies": {
		"@alexaegis/tsconfig": "*"
	},
	"devDependencies": {
		"@types/fs-extra": "^9.0.13",
		"typescript": "4.8.4",
		"@schemastore/package": "^0.0.6"
	},
	"exports": {
		"./static/example": "./static/example.json",
		"./base-vite.config": {
			"default": "./src/base-vite.config.ts"
		},
		".": {
			"default": "./src/index.ts"
		},
		"./library-vite.config": {
			"default": "./src/library-vite.config.ts"
		},
		"./svelte-vite.config": {
			"default": "./src/svelte-vite.config.ts"
		}
	},
	"bin": {
		"distribute-vite-config": "./src/bin/distribute-vite-config.ts",
		"alexaegis-vite-postinstall": "./src/bin/postinstall.ts"
	}
}
