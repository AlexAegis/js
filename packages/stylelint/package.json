	},
	"keywords": [
		"stylelint",
		"ts",
		"typescript",
		"js",
		"javascript"
	],
	"private": false,
	"config": {
		"engine-strict": false
	},
	"scripts": {
		"format": "prettier --write .",
		"lint:es": "eslint --max-warnings=0 --fix .",
		"lint:format": "prettier --check ."
	},
	"sideEffects": false,
	"publishConfig": {
		"access": "public"
	},
	"dependencies": {
		"stylelint": "14.15.0"
	},
	"devDependencies": {
		"@types/node": "^18.11.9",
		"typescript": "4.9.3",
		"@alexaegis/vite": "*"
	}
}
