{
	"name": "@alexaegis/stylelint",
	"description": "My stylelint config",
	"version": "0.0.1",
	"license": "MIT",
	"author": {
		"email": "alexaegis@gmail.com",
		"name": "AlexAegis",
		"url": "https://www.github.com/AlexAegis"
	},
	"homepage": "https://www.github.com/AlexAegis/js#readme",
	"repository": "github:AlexAegis/js",
	"bugs": {
		"url": "https://www.github.com/AlexAegis/js/issues"
	},
	"keywords": [
		"stylelint",
		"ts",
		"typescript",
		"js",
		"javascript"
	],
	"private": false,
	"config": {
		"engine-strict": false
	},
	"scripts": {
		"format": "prettier --write .",
		"lint:es": "eslint --max-warnings=0 .",
		"lint:format": "prettier --check ."
	},
	"sideEffects": false,
	"publishConfig": {
		"access": "public"
	},
	"dependencies": {
		"stylelint": "14.14.0"
	},
	"devDependencies": {
		"typescript": "4.8.4",
		"vite": "3.2.1",
		"vite-plugin-static-copy": "0.11.0"
	}
}
